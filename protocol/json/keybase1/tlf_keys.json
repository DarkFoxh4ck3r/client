{
  "protocol": "tlfKeys",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    },
    {
      "path": "identify.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "enum",
      "name": "TLFIdentifyBehavior",
      "symbols": [
        "DEFAULT_KBFS_1",
        "CHAT_2"
      ]
    },
    {
      "type": "record",
      "name": "CanonicalTlfName",
      "fields": [],
      "typedef": "string"
    },
    {
      "type": "record",
      "name": "CryptKey",
      "fields": [
        {
          "type": "int",
          "name": "KeyGeneration"
        },
        {
          "type": "Bytes32",
          "name": "Key"
        }
      ]
    },
    {
      "type": "record",
      "name": "TLFBreak",
      "fields": [
        {
          "type": "string",
          "name": "username"
        },
        {
          "type": "IdentifyTrackBreaks",
          "name": "breaks"
        }
      ]
    },
    {
      "type": "record",
      "name": "GetTLFCryptKeysRes",
      "fields": [
        {
          "type": "TLFID",
          "name": "tlfID"
        },
        {
          "type": "CanonicalTlfName",
          "name": "CanonicalName"
        },
        {
          "type": {
            "type": "array",
            "items": "CryptKey"
          },
          "name": "CryptKeys"
        },
        {
          "type": {
            "type": "array",
            "items": "TLFBreak"
          },
          "name": "breaks"
        }
      ]
    },
    {
      "type": "record",
      "name": "CanonicalTLFNameAndIDWithBreaks",
      "fields": [
        {
          "type": "TLFID",
          "name": "tlfID"
        },
        {
          "type": "CanonicalTlfName",
          "name": "CanonicalName"
        },
        {
          "type": {
            "type": "array",
            "items": "TLFBreak"
          },
          "name": "breaks"
        }
      ]
    }
  ],
  "messages": {
    "getTLFCryptKeys": {
      "request": [
        {
          "name": "tlfName",
          "type": "string"
        },
        {
          "name": "identifyBehavior",
          "type": "TLFIdentifyBehavior"
        }
      ],
      "response": "GetTLFCryptKeysRes",
      "doc": "getTLFCryptKeys returns TLF crypt keys from all generations and the TLF ID.\n   TLF ID should not be cached or stored persistently."
    },
    "getPublicCanonicalTLFNameAndID": {
      "request": [
        {
          "name": "tlfName",
          "type": "string"
        },
        {
          "name": "identifyBehavior",
          "type": "TLFIdentifyBehavior"
        }
      ],
      "response": "CanonicalTLFNameAndIDWithBreaks",
      "doc": "getPublicCanonicalTLFNameAndID return the canonical name and TLFID for tlfName.\n   TLF ID should not be cached or stored persistently."
    }
  },
  "namespace": "keybase.1"
}